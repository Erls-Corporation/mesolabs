{
  "name": "Stalkr Chrome Extension",
  "version": "1.0",
  "description": "With this Extension, you can stalk who you follow, and vice versa.",
  "options_page": "options.html",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["contentscript.js"],
    "run_at": "document_end"
  }],
  "background_page": "background.html",
  "permissions": [
    "tabs",
    "http://sakura.mesolabs.com/*",
    "https://api.twitter.com/*",
    "http://api.twitter.com/*"
  ]
}
