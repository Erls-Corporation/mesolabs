script(type='text/javascript')
  $(function() {
    var userId = $('#userId').text().trim();
    console.log(userId);
    var socket = io.connect('http://mesolabs.no.de/' + userId);
    socket.on('access', function(data) {
      if ($('.page').size() > 30) {
        $('.page:last').remove();
      }
      var user = data.user;
      $('#pages').prepend(
        '<p class="page">' +
          '<a href="/' + user.name + '">' +
            '<img src="' + user.profile_url + '"/>' + 
          '</a>' +
          '<span style="font-weight: bold">' + data.title + '</span> ' +
          '(<a href="' + data.url + '">' + data.url + '</a>)' +
        '</p>');
    });
  });
#userId(style='display: none;')= userId
h2= name
 |&nbsp;is accessing these pages NOW.
#pages
